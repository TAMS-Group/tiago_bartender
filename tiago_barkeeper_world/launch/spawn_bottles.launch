<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/red_bottle.xacro"/>
    <arg name="model_name" value="botle_1"/>
    <arg name="z" value="0.79"/>
    <arg name="x" value="2.45"/>
  </include>
  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/orange_bottle.xacro"/>
    <arg name="model_name" value="botle_2"/>
    <arg name="z" value="0.79"/>
    <arg name="x" value="2.45"/>
    <arg name="y" value="0.5"/>
  </include>
  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/yellow_bottle.xacro"/>
    <arg name="model_name" value="botle_3"/>
    <arg name="z" value="0.79"/>
    <arg name="x" value="2.45"/>
    <arg name="y" value="-0.5"/>
  </include>

  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/yellow_glass.xacro"/>
    <arg name="model_name" value="glass_1"/>
    <arg name="z" value="0.9"/>
    <arg name="x" value="0.0"/>
    <arg name="y" value="0.0"/>
  </include>
  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/yellow_glass.xacro"/>
    <arg name="model_name" value="glass_2"/>
    <arg name="z" value="0.9"/>
    <arg name="x" value="0.0"/>
    <arg name="y" value="-1.0"/>
  </include>
  <include file="$(find tiago_barkeeper_world)/launch/spawn_xacro.launch">
    <arg name="xacro_file" value="$(find tiago_barkeeper_world)/robots/yellow_glass.xacro"/>
    <arg name="model_name" value="glass_3"/>
    <arg name="z" value="0.9"/>
    <arg name="x" value="0.0"/>
    <arg name="y" value="1.0"/>
  </include>
</launch>
